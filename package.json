{
  "name": "sl",
  "version": "0.1.0",
  "bin": {
    "sl": "bin/sl.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "synth": "cdk synth SlStack --no-staging > template.yaml",
    "poller": "sam local invoke VodPoller741EDAA0 --env-vars env.json --no-event",
    "clipfinder": "sam local invoke ClipFinderA73374EE --env-vars env.json --event ./events/clipFinderEvent.json",
    "downloader": "sam local invoke DownloadHandler09CC40DB --env-vars env.json --event ./events/event.json",
    "cccfinder": "sam local invoke CCCFinder4C010E91 --env-vars env.json --event ./events/cccFinderEvent.json",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.95.1",
    "@aws-cdk/yaml-cfn": "^1.95.1",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "aws-cdk": "^1.95.1",
    "aws-sdk": "^2.874.0",
    "cdk-assets": "^1.95.1",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.95.1",
    "@aws-cdk/aws-codepipeline": "^1.95.1",
    "@aws-cdk/aws-codepipeline-actions": "^1.95.1",
    "@aws-cdk/aws-events-targets": "^1.95.1",
    "@aws-cdk/aws-lambda-event-sources": "^1.95.1",
    "@aws-cdk/aws-lambda-nodejs": "^1.95.1",
    "@aws-cdk/aws-lambda-python": "^1.96.0",
    "@aws-cdk/aws-s3": "^1.95.1",
    "@aws-cdk/aws-secretsmanager": "^1.95.1",
    "@aws-cdk/aws-sns-subscriptions": "^1.95.1",
    "@aws-cdk/core": "1.95.1",
    "@aws-cdk/pipelines": "^1.95.1",
    "fetch": "^1.1.0",
    "mongodb": "^3.6.6",
    "mongodb-client-encryption": "^1.2.3",
    "node-fetch": "^2.6.1",
    "source-map-support": "^0.5.16"
  }
}
